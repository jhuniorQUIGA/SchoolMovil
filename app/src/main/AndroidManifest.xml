<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icar.demy.schoolmovil">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SchoolMovil"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actividades.Estudiante.ActividadEstudianteActivity"></activity>
        <activity android:name=".actividades.Profesor.ObservacionesProfesorActivity" />
        <activity android:name=".actividades.Director.AdminRegisTutorActivity" />
        <activity android:name=".actividades.Director.AdminRegisEstudianteActivity" />
        <activity android:name=".actividades.Director.AdminRegisProfeActivity" />
        <activity android:name=".actividades.Inicio.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>